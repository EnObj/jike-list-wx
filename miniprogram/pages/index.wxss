.date-list-item{
  padding:5px;width:100px;text-align:center;border-radius:5px;
}
.current-date-item{
  background: red;color:white;
}
.channel-list-item{
  padding:5px;width:70px;text-align:center;
}
.current-channel-item{
  border-bottom:2px blue solid;
}
.program-list-item{
  display:flex;padding:10px;
}
.program-list-item-odd{
  background: #eee;
}
.program-status{
  padding:10px;border-radius:50%;font-size:12px;text-align:center;
  color: white;
}
.program-status-over{
  background:gray;
}
.program-status-curr{
  background:red;
}
.program-status-fulture{
  background:green;
}
.channel-date{
  position: sticky;
  top: 0;
  background: #f6f6f6;
}